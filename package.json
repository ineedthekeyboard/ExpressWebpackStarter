{
  "name": "sampleexpress",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "buildClientProd": "npm run cleanClient && webpack -p --config webpack.prod.config.js --progress",
    "buildClientDev": "npm run cleanClient && webpack --config webpack.dev.config.js",
    "cleanClient": "rimraf public/*",
    "startDev": "npm run buildClientDev && node server.js",
    "startProd": "npm run buildClientProd && node server.js"
  },
  "dependencies": {
    "body-parser": "~1.15.2",
    "cookie-parser": "~1.4.3",
    "debug": "~2.2.0",
    "express": "~4.14.0",
    "hbs": "~4.0.1",
    "morgan": "~1.7.0",
    "node-sass-middleware": "0.9.8",
    "serve-favicon": "~2.3.0"
  },
  "devDependencies": {
    "ajv": "^5.2.2",
    "autoprefixer": "^7.1.2",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "css-loader": "^0.28.4",
    "es6-promise": "^4.1.1",
    "file-loader": "^0.11.2",
    "node-sass": "^4.5.3",
    "nodemon": "^1.11.0",
    "postcss": "^6.0.7",
    "postcss-loader": "^2.0.6",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "stylelint": "^8.0.0",
    "webpack": "^3.3.0",
    "webpack-dev-middleware": "^1.11.0",
    "webpack-hot-middleware": "^2.18.2"
  }
}
